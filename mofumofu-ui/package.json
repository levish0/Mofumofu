{
	"name": "mofumofu-ui",
	"private": true,
	"version": "2.4.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "wrangler dev .svelte-kit/cloudflare/_worker.js --port 4173",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"fmt": "prettier --write .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"unlighthouse:dev": "pnpm dlx unlighthouse --site http://localhost:5173",
		"unlighthouse:preview": "pnpm dlx unlighthouse --site http://localhost:8787",
		"gen": "wrangler types",
		"generate:api": "openapi-typescript swagger.json -o src/lib/api/generated.d.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.561.0",
		"@playwright/test": "^1.58.1",
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^24",
		"@vitest/browser-playwright": "^4.0.18",
		"@zxcvbn-ts/core": "^3.0.4",
		"@zxcvbn-ts/language-common": "^3.0.4",
		"@zxcvbn-ts/language-en": "^3.0.2",
		"bits-ui": "^2.15.5",
		"clsx": "^2.1.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.3.0",
		"highlight.js": "^11.11.1",
		"jsrepo": "^3.2.1",
		"ky": "^1.14.3",
		"openapi-typescript": "^7.12.0",
		"playwright": "^1.58.1",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"rehype-highlight": "^7.0.2",
		"rehype-katex": "^7.0.1",
		"rehype-raw": "^7.0.0",
		"rehype-sanitize": "^6.0.0",
		"rehype-slug": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark-breaks": "^4.0.0",
		"remark-emoji": "^5.0.2",
		"remark-gfm": "^4.0.1",
		"remark-github-blockquote-alert": "^2.0.1",
		"remark-math": "^6.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"rollup-plugin-visualizer": "^6.0.5",
		"runed": "^0.37.1",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"svelte-easy-crop": "^5.0.0",
		"svelte-hero-icons": "^5.2.0",
