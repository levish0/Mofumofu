{
  "name": "mofumofu-markdown-service",
  "version": "2.1.0",
  "author": "Levi Laine <shiueo.csh@gmail.com>",
  "repository": "https://github.com/levish0/mofumofu",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "fmt": "bunx prettier --write src/",
    "fmt:check": "bunx prettier --check src/"
  },
  "dependencies": {
    "elysia": "latest",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "^5.0.2",
    "remark-gfm": "^4.0.1",
    "remark-github-blockquote-alert": "^2.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prettier": "^3.8.1"
  },
  "module": "src/index.ts"
}
